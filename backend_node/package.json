{
  "name": "rotracker_server",
  "version": "1.0.0",
  "description": "This server will run the script to log player data to be sent to the extension",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "build": "pkg ."
  },
  "bin": "./server.js",
  "pkg": {
    "assets": [
      ".env"
    ],
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.3"
  }
}
